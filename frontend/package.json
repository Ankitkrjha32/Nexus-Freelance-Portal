{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently -n \"frontend,backend\" -c \"bgBlue,bgYellow\" \"npm run start:host\" \"npm run server\"",
        "server": "cd ../backend && npm run dev",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "start": "vite",
        "start:host": "vite --host --port 3000"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.9.2",
        "@tailwindcss/vite": "^4.1.4",
        "axios": "^1.6.5",
        "dotenv": "^17.2.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-icons": "^4.12.0",
        "react-redux": "^9.2.0",
        "react-router-dom": "^6.21.1",
        "tailwindcss": "^4.1.4"
    },
    "devDependencies": {
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.3",
        "@vitejs/plugin-react-swc": "^3.9.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.25.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "vite": "^6.3.4"
    }
